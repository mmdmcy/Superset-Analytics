{
  "charts": [
    {
      "chart_id": 1,
      "chart_type": "bar",
      "title": "Sales by Region",
      "data_source": "sales_data",
      "x_axis": "region",
      "y_axis": "total_sales",
      "filters": {
        "date_range": "last_year"
      },
      "options": {
        "color_scheme": "blue",
        "show_legend": true
      }
    },
    {
      "chart_id": 2,
      "chart_type": "line",
      "title": "Monthly Active Users",
      "data_source": "user_data",
