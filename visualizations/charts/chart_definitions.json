      "y_axis": "active_users",
      "filters": {
        "user_type": "registered"
      },
      "options": {
        "line_style": "solid",
        "show_grid": true
      }
    },
    {
      "chart_id": 3,
      "chart_type": "pie",
      "title": "Market Share by Product",
      "data_source": "market_data",
